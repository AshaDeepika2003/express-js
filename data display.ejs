<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <table border="1">
      <tr>
        <th>reg_no</th>
        <th>name</th>
        <th>email</th>
        <th>mobile</th>
      </tr>
      <% rows.forEach(row)=>{ %>
        <tr>
          <td><%=row.reg_no %></td>
          <td><%=row.name %></td>
          <td><%=row.email %></td>
          <td><%row.mobile %></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
